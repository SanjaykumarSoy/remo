#include <stdio.h>
#include<math.h>
int main() {
    double a=5, b=2,c;
    c=sqrt(a*a+b*b);
    printf("the length of hypotenuse is %.2lf\n",c);
   
    return 0;
}

#include<stdio.h>
int main(){
    int date=24, month= 01 , year=2024;
    if(year%400==0){
        printf("%d/%d/%d is leap year ",date ,month,year);
    }
    else if(year%100==0){
        printf("%d/%d/%d is not a leap year",date, month, year);
    }
    else if(year%4==0){
        printf("%d/%d/%d is a leap year", date ,month,year);
    }
    else{
        printf("%d/%d/%d is not a leap year", date, month, year);
    }
    return 0;
}
